# OpenAI (required for embeddings)
OPENAI_API_KEY=""
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
# Optional: OpenAI "dimensions" parameter (defaults to EMBEDDING_DIM if omitted)
OPENAI_EMBEDDING_DIM=1536

# Qdrant (where vectors + item metadata are stored)
QDRANT_URL=http://127.0.0.1:6333
QDRANT_COLLECTION_ITEMS=content_items_v1
QDRANT_COLLECTION_CHUNKS=content_chunks_v1
# Optional deterministic UUIDv5 namespace for point IDs (if omitted, scripts use an internal default)
QDRANT_NAMESPACE_UUID=

# Content root override (recommended when content lives in a sibling private repo)
# Example: ../resume_web_content/ui/src/content
RESUME_UI_CONTENT_ROOT=

# Optional performance knobs
OPENAI_EMBEDDING_BATCH=64
QDRANT_BATCH=128

